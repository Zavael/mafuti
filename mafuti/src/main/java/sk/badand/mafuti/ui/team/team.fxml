<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sk.badand.mafuti.ui.team.TeamController">
    <stylesheets>
        <URL value="@/styles/team.css" />
    </stylesheets>
    <children>
        <Label text="%lbl.team" />
        <TabPane tabClosingPolicy="UNAVAILABLE" VBox.vgrow="ALWAYS" >
            <tabs>
                <Tab text="%tab.players">
                    <content>
                        <AnchorPane>
                            <children>
                                <BorderPane prefHeight="200.0" prefWidth="200.0">
                                    <center>
                                        <ListView prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
                                    </center>
                                </BorderPane>
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
                <Tab text="%tab.formation">
                    <content>
                        <AnchorPane>
                            <children>
                                <BorderPane prefHeight="200.0" prefWidth="200.0">
                                    <center>
                                        <ListView prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
                                    </center>
                                </BorderPane>
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
                <Tab text="%tab.tactics">
                    <content>
                        <AnchorPane>
                            <children>
                                <BorderPane prefHeight="200.0" prefWidth="200.0">
                                    <center>
                                        <ListView prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
                                    </center>
                                </BorderPane>
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
            </tabs>
        </TabPane>
    </children>
</VBox>
