<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
      AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"
      fx:controller="sk.badand.mafuti.ui.editor.intro.IntroPresenter"
      fx:id="rootPane" alignment="CENTER">
	<Button mnemonicParsing="false" onAction="#createGame" text="%btn.new" styleClass="biggerFont"/>
	<Button mnemonicParsing="false" onAction="#loadGame" text="%btn.load"/>
	<Button mnemonicParsing="false" text="%btn.exit"/>
</VBox>
