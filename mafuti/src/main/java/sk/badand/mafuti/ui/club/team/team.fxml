<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sk.badand.mafuti.ui.club.team.TeamPresenter">
    <children>
        <Label fx:id="title" styleClass="header1" text="%lbl.heading" />
        <TabPane tabClosingPolicy="UNAVAILABLE" VBox.vgrow="ALWAYS">
            <tabs>
                <Tab closable="false" text="%tab.players">
                    <content>
                        <AnchorPane fx:id="tabPlayers" />
                    </content>
                </Tab>
                <Tab closable="false" text="%tab.formation">
                    <content>
                        <AnchorPane fx:id="tabFormation" />
                    </content>
                </Tab>
                <Tab closable="false" text="%tab.tactics">
                    <content>
                        <AnchorPane fx:id="tabTactics" />
                    </content>
                </Tab>
            </tabs>
        </TabPane>
    </children>
</VBox>
